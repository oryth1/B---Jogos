local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Configuração da interface
local Window = Rayfield:CreateWindow({
    Name = "Oryh Hub | Universal BR | Blade Ball", 
    LoadingTitle = "Carregando Blade Ball",
    LoadingSubtitle = "Por favor, aguarde...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "OryhHubConfigs", -- Pasta para salvar configurações
        FileName = "UniversalScript"
    },
    Discord = {
        Enabled = false,
        Invite = "", -- Adicione um convite do Discord se necessário
        RememberJoins = true
    },
    KeySystem = false -- Sistema de chave desativado
})

-- Função para criar botões que executam scripts
local function AddScriptButton(tab, name, url)
    tab:CreateButton({
        Name = name,
        Callback = function()
            loadstring(game:HttpGet(url))()
            Rayfield:Notify({
                Title = "Oryh Hub",
                Content = name .. " carregado com sucesso!",
                Duration = 5
            })
        end
    })
end

-- >[Início]<

local InicioTab = Window:CreateTab("Início")

AddScriptButton(InicioTab, "Bakugan Hub", "https://raw.githubusercontent.com/SoyAdriYT/Bakugan/refs/heads/main/Bakugan.lua")
AddScriptButton(InicioTab, "FrostWare Hub", "https://raw.githubusercontent.com/Fsploit/Frostware-/refs/heads/main/Bypass.lua")
AddScriptButton(InicioTab, "Plutonium Hub", "https://raw.githubusercontent.com/PawsThePaw/Plutonium.AA/main/Plutonium.Loader.lua")
AddScriptButton(InicioTab, "Fluxus Z Hub", "https://raw.githubusercontent.com/Fsploit/Flux-Z-Beta/refs/heads/main/Flux-Z-Beta-Blade-Ball.lua%22%29%29%28%29")
AddScriptButton(InicioTab, "PitHubll X Hub", "https://raw.githubusercontent.com/SoyAdriYT/PitbullHubX/refs/heads/main/PitbullHubX.lua%22")
AddScriptButton(InicioTab, "Waver Hub", "https://raw.githubusercontent.com/JoshSheesh/Waver-Full-Script/refs/heads/main/Protected_7926735646342814.txt%22%29%29%28%29")
AddScriptButton(InicioTab, "Seeds Hub", "https://raw.githubusercontent.com/lolditto-q-u-m-u-u-qwq-idk/Seeds-NEXT--LOL/main/auto_parry___lol____________idk.lua%22%29%29%28%29")
AddScriptButton(InicioTab, "Fade V3 Hub", "https://paste.gg/p/anonymous/e205c0dabb0a4e8ebcfbe0af34934280/files/bf814e270c334ddb8b4e79f664279499/raw")
AddScriptButton(InicioTab, "Blade Ball Hub", "https://rawscripts.net/raw/Blade-Ball-Yuhhh-Yuhh-Free-Yuhhhhh-17457")
AddScriptButton(InicioTab, "Lunar Hub", "https://raw.githubusercontent.com/Akirascripts/Lunar/refs/heads/main/LuanrOnTop")





-- Mensagem inicial
Rayfield:Notify({
    Title = "Blade Ball",
    Content = "Carregando scripts...",
    Duration = 5
})
