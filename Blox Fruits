local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Configuração da interface
local Window = Rayfield:CreateWindow({
    Name = "Oryh Hub | Universal BR | Blox Fruits", 
    LoadingTitle = "Carregando Blox Fruits",
    LoadingSubtitle = "Por favor, aguarde...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "OryhHubConfigs", -- Pasta para salvar configurações
        FileName = "UniversalScript"
    },
    Discord = {
        Enabled = false,
        Invite = "", -- Adicione um convite do Discord se necessário
        RememberJoins = true
    },
    KeySystem = false -- Sistema de chave desativado
})

-- Função para criar botões que executam scripts
local function AddScriptButton(tab, name, url)
    tab:CreateButton({
        Name = name,
        Callback = function()
            loadstring(game:HttpGet(url))()
            Rayfield:Notify({
                Title = "Oryh Hub",
                Content = name .. " carregado com sucesso!",
                Duration = 5
            })
        end
    })
end

-- >[Blox Fruits]<

local BloxFruitsTab = Window:CreateTab("Blox Fruits")

AddScriptButton(BloxFruitsTab, "Alchemy Hub", "https://scripts.alchemyhub.xyz")
AddScriptButton(BloxFruitsTab, "Speed X Hub", "https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua")
AddScriptButton(BloxFruitsTab, "Kncryptc Hub", "https://raw.githubusercontent.com/3345-catsus/Kncrypt/refs/heads/main/sources/BloxFruit.lua")
AddScriptButton(BloxFruitsTab, "HOHO Hub", "https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI")
AddScriptButton(BloxFruitsTab, "Thunder Z Hub", "https://raw.githubusercontent.com/ThundarZ/Welcome/refs/heads/main/Main/Loader/AllGame.lua")
AddScriptButton(BloxFruitsTab, "Redz Hub", "https://raw.githubusercontent.com/realredz/BloxFruits/refs/heads/main/Source.lua")
AddScriptButton(BloxFruitsTab, "Xero Hub", "https://apixerohub.x10.mx/main.luarmor")
AddScriptButton(BloxFruitsTab, "Raito Hub", "https://raw.githubusercontent.com/Efe0626/RaitoHub/refs/heads/main/Script")
AddScriptButton(BloxFruitsTab, "Banana Hub", "https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/BananaHub.lua")
AddScriptButton(BloxFruitsTab, "Cokka Hub", "https://raw.githubusercontent.com/UserDevEthical/Loadstring/main/CokkaHub.lua")
AddScriptButton(BloxFruitsTab, "Mukuro Hub", "https://auth.quartyz.com/scripts/Loader.lua")
AddScriptButton(BloxFruitsTab, "Min Hub", "https://raw.githubusercontent.com/Zunes-Bypassed/NOPE/main/Min.lua")
-- Mensagem inicial
Rayfield:Notify({
    Title = "Blox Fruits",
    Content = "Carregando scripts...",
    Duration = 5
})
